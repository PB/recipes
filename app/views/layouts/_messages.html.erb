<% if notice %>
  <div id="messages"  class="container spacer-top">
    <br>
    <div class="row">
      <div class="col-md-10">
          <div class="alert alert-success">
            <%= notice %>
            <a class="close" data-dismiss="alert">&#215;</a>
          </div>
      </div>
    </div>
  </div>
<% end %>